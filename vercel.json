{
    "version": 2,
    "functions": {
        "api/chat.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/chat",
            "dest": "/api/chat.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/chat.py"
        }
    ]
} 